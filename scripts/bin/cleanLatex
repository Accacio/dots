#!/bin/bash
touch dummy.dummy
find .  -type f -not -name '*.tex'\
     -not -name '*.org' \
     -not -name '*.dot' \
     -not -name '*.exe' \
     -not -name '*.sty' \
     -not -name '*.cls' \
     -not -name '*.bib' \
     -not -name '*.bat' \
     -not -name '*.sh' \
     -not -name '*.pdf' \
     -not -name '*.projectile'\
     -not -name 'makefile'\
     -not -name '*.ist'\
     -not -name '*.bst'\
     -not -wholename '*/auto/*'\
     | xargs rm --

