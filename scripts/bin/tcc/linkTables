#!/bin/bash
#complete
sed -i -E "s;c([tp])(t|)([0-9]+)(|m[0-9]+);\\\hyperlink{completeNet:\1\2\3}{\\\hypertarget{completeTable:\1\2\3\4}{$\1_{\3}$}};g" $1
# #partial
sed -i -E "s;([tp])(t|)([0-9]+)(|m[0-9]+)(\ *&);\\\hyperlink{partialNet:\1\2\3}{\\\hypertarget{partialTable:\1\2\3\4}{$\1_{\3}$}}\5;g" $1


